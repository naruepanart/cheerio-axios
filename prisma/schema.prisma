generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Users {
  id    Int     @id @default(autoincrement())
  email String
  name  String
  posts Posts[]
}

model Posts {
  id        Int     @id @default(autoincrement())
  title     String
  content   String
  published Boolean @default(false)
  users     Users   @relation(fields: [usersId], references: [id])
  usersId   Int
}
